        storage:       "ConfigStorage"
    }
    isr_metadata: {
        template:      "%(visit)07d/metadata/metadata-%(visit)07d_%(ccdnum)02d.boost"
        python:        "lsst.daf.base.PropertySet"
        persistable:   "PropertySet"
        storage:       "BoostStorage"
        tables:        raw
    }
    characterizeImage_metadata: {
        template:      "%(visit)07d/metadata/metadata-%(visit)07d_%(ccdnum)02d.boost"
        python:        "lsst.daf.base.PropertySet"
