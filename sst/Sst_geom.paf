Camera: {
    name: "SST"
    serial: 1
    nCol: 1
    nRow: 1
    Raft: {
        name: "R:0,0"
        serial: -1
        index: 0 0
        offset: 0.0 0.0
    }
    Distortion: {
        # No idea yet
    }
}
Amp: {
    ptype: "default"
    datasec: 0 0 512 4096
    #biassec: 512 0 XXX 4096 # Data have no bias section
    ewidth: 512
    eheight: 4096
}
Ccd: {
    ptype: "default"
    pixelSize: 1.0 # All positions in pixels, rather than mm
    nCol: 4
    nRow: 1
    Amp: {
        serial: 1
        ptype: "default"
        flipLR: true
        nQuarter: 0
        hdu: 0
        diskCoordSys: "camera"
        index: 0 0
    }
    Amp: {
        serial: 0
        ptype: "default"
        flipLR: false
        nQuarter: 0
        hdu: 0
        diskCoordSys: "camera"
        index: 1 0
    }
    Amp: {
        serial: 3
        ptype: "default"
        flipLR: true
        nQuarter: 0
        hdu: 0
        diskCoordSys: "camera"
        index: 2 0
    }
    Amp: {
        serial: 4
        ptype: "default"
        flipLR: false
        nQuarter: 0
        hdu: 0
        diskCoordSys: "camera"
        index: 3 0
    }
}
